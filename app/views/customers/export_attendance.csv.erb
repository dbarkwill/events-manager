<% headers = ['Customer Number', 'Full Name', 'Registered at Event', 'Dinner Count'] %>
<%= CSV.generate_line headers %>
<% @customers.each do |customer| %>
  <%= CSV.generate_line([customer.customer_number, customer.co, customer.name, customer.registered_at_event, customer.dinner_count, customer.actual_dinner_count]) %>
<% end %>
